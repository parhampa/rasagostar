<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.rasagostar" version="1.3.52" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <name>رساگستر</name>
    <description>
        rasa gostar project
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        0004320.ir
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*0004320.ir/*" />
    <allow-intent href="http://*shaparak.ir/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="tg://:*" />
    <allow-intent href="whatsapp://*" />
    <access launch-external="yes" origin="tel:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
