<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.rasagostar" version="1.3.52" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>رساگستر</name>
    <description>
        rasa gostar project
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        0004320.ir
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*0004320.ir/*" />
    <allow-intent href="http://*shaparak.ir/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="tg://:*" />
    <allow-intent href="whatsapp://*" />
    <access launch-external="yes" origin="tel:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
    <plugin name="cordova-plugin-inappbrowser" spec="^3.0.0" />
    <engine name="android" spec="^7.1.4" />
</widget>
